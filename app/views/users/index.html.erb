<p>Find me in app/views/users/index.html.erb</p>

<h1>Listing Users</h1>

<table>
  <thead>
  <tr>
    <th>Name</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
      </tr>
  <% end %>
  </tbody>
</table>

